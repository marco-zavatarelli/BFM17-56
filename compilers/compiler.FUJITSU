# Set options for the Fujitsu compiler - on Linux/Intel and SunOS.
ifeq ($(FORTRAN_COMPILER),FUJITSU)
FC=frt
can_do_F90=true
DEFINES += -DFORTRAN95
MODULES=-Am -M$(MODDIR)
EXTRAS  = -ml=cdecl -fw
EXTRAS  = -fw
DEBUG_FLAGS = -g -H aeus
PROF_FLAGS  = -pg -O3
PROD_FLAGS  = -O -K fast
REAL_4B	= real\(4\)
endif
