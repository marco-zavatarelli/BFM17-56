# Set options for the Intel Fortran 95 compiler - ver. 8.
ifeq ($(FORTRAN_COMPILER),IFORT)
FC=ifort
DEFINES += -DFORTRAN95
can_do_F90=true
F90_to_f90=
MODULES=
MODULES=-module $(MODDIR)
EXTRAS  = -w95 
#EXTRAS  = -w95 -e95
DEBUG_FLAGS = -g -C -traceback -check bounds
PROF_FLAGS  = -qp -p
PROD_FLAGS  = -O3
REAL_4B = real\(4\)
endif
