	Doi = {10.1029/2009JC005950},
	Isbn = {0148-0227},
	Journal = jgr,
	Keywords = {Mediterranean; EMT; modeling; 4513 Oceanography: Physical: Decadal ocean variability; 4504 Oceanography: Physical: Air/sea interactions; 4255 Oceanography: General: Numerical modeling},
	Month = {08},
	Number = {C8},
	Pages = {C08017},
	Publisher = {AGU},
	Title = {Modeling the {M}editerranean {S}ea interannual variability during 1961-2000: Focus on the {E}astern {M}editerranean {T}ransient},
	Ty = {JOUR},
