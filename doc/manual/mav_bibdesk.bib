	Doi = {10.1029/2002JD002670},
	Journal = jgr,
	Keywords = {1610 Global Change: Atmosphere; 1635 Global Change: Oceans; 1827 Hydrology: Glaciology; 3339 Meteorology and Atmospheric Dynamics: Ocean/atmosphere interactions},
	Month = {07},
