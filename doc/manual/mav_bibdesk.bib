	Journal = {Applied Optics},
	Keywords = {OCEAN COLOR; PHYTOPLANKTON ABSORPTION; SATELLITE RETRIEVAL; SHALLOW WATERS; CHLOROPHYLL-A; MODEL; INHERENT; OPTIMIZATION; COEFFICIENTS; FEASIBILITY},
	Month = {May},
