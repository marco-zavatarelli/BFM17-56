	Journal = meps,
	Keywords = {Biogeography},
	Local-Url = {file://localhost/Users/vichi/Bibliography/mcquatters-gollop07.pdf},
	Pages = {301--306},
	Publication-Type = {J},
	Title = {Spatial patterns of diatom and dinoflagellate seasonal cycles in the NE Atlantic Ocean},
	Volume = {339},
	Year = {2007},
