@article{wetzel05,
	Abstract = {Trends and variability in the ocean-atmosphere CO2 flux and the uptake of anthropogenic CO2 are simulated for the period 1948\&{\#}8211;2003, using a biogeochemical carbon cycle model (HAMOCC5) coupled online to a global ocean general circulation model (MPI-OM). The model is forced by daily National Centers for Environmental Prediction/National Center for Atmospheric Research (NCEP/NCAR) reanalysis data from 1948 to 2003. We find a global interannual variability of $\pm$0.50 PgC yr\&{\#}8722;1 (2\&{\#}963;) which is largely dominated by ocean dynamics in the equatorial Pacific. On decadal scale, two patterns are of global importance. First, the regime shift that occurs in 1975\&{\#}8211;1977 when the modeled interannual variability of the equatorial Pacific changes from $\pm$0.32 PgC yr\&{\#}8722;1 to $\pm$0.23 PgC yr\&{\#}8722;1 and the mean outgasing of CO2 decreases from 0.70 PgC yr\&{\#}8722;1 to 0.58 PgC yr\&{\#}8722;1. Second, the trend that occurs in the Southern Ocean, where we find an increase of the CO2 fluxes over the full 56-year simulation period. The flux results from stronger upwelling and deeper mixed layers which are caused by increasing wind velocities. We estimate an average CO2 flux into the ocean of 1.49 PgC yr\&{\#}8722;1 for 1980\&{\#}8211;1989 and 1.74 PgC yr\&{\#}8722;1 for 1990\&{\#}8211;1999, with extremes of 1.20 PgC yr\&{\#}8722;1 at the La Ni{\`O}a event in 1996 and 2.10 PgC yr\&{\#}8722;1 during the El Ni{\`O}o events in 1993 and 1998. Because of the rising buffer factor, the uptake of anthropogenic CO2 is slowing down in regions of shallow mixing toward the end of the simulation. Overall, about 124 Pg of anthropogenic carbon has accumulated in the model ocean until the end of 2003.},
	Author = {Wetzel, P. and Winguth, A. and Maier-Reimer, E.},
	Date = {2005/04/06},
	Date-Added = {2011-02-05 16:59:42 +0100},
	Date-Modified = {2011-02-05 17:00:15 +0100},
	Doi = {10.1029/2004GB002339},
	Isbn = {0886-6236},
	Journal = gbc,
