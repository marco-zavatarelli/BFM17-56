	Isbn = {0886-6236},
	Journal = gbc,
	Keywords = {Remote sensing; phytoplankton; climatology; 0480 Biogeosciences: Remote sensing; 4855 Oceanography: Biological and Chemical: Phytoplankton; 9325 Geographic Location: Atlantic Ocean; 4223 Oceanography: General: Descriptive and regional oceanography},
	Month = {07},
