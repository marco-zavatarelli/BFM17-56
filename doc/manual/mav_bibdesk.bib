	Journal = {Eos Trans. AGU},
	Keywords = {tropical Pacific climate change; ENSO; 1637 Global Change: Regional climate change; 0473 Biogeosciences: Paleoclimatology and paleoceanography (3344, 4900); 4922 Paleoceanography: El Nino (4522); 1626 Global Change: Global climate models (3337, 4928); 1630 Global Change: Impacts of global change (1225)},
	Number = {16},
