	Keywords = {Mediterranean sea; Modeling; River discharge; Environmental impact; Thermohaline circulation; Deep water formation},
	Number = {1-4},
	Pages = {121-143},
	Publisher = {Elsevier Science},
	Title = {Impacts of the {N}ile {R}iver damming on the thermohaline circulation and water mass characteristics of the Mediterranean Sea},
	Volume = {52},
	W = {/cgi-bin/sciserv.pl?collection=journals&journal=09247963&issue=v52i1-4&article=121_iotnrdmcotms},
